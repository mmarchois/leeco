{% set activeClass = 'text-blue-600 border-blue-600 dark:text-blue-500 dark:hover:text-blue-500 dark:border-blue-500' %}
{% set defaultClass = 'border-gray-50 dark:border-gray-800 hover:text-gray-600 hover:border-gray-300 dark:hover:border-gray-400 dark:hover:text-gray-300 text-gray-500 dark:text-gray-400' %}

<div class="border-t border-b border-gray-100 bg-gray-50 dark:border-gray-800 dark:bg-gray-800 mb-4">
    <div class="w-full px-3 mx-auto max-w-8xl lg:px-3">
        <ul class="flex flex-wrap -mb-px -ml-4">
            <li class="mr-2">
                <a href="{{ path('app_events_dashboard', { eventUuid }) }}" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if 'app_events_dashboard' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true" class="w-5 h-5">
                            <path d="M5 3a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2V5a2 2 0 00-2-2H5zM5 11a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2v-2a2 2 0 00-2-2H5zM11 5a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2V5zM11 13a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2v-2z"></path>
                        </svg>
                    </span>
                    {{ 'event.dashboard'|trans }}
                </a>
            </li>
            <li class="mr-2">
                <a href="{{ path('app_guests_list', { eventUuid }) }}" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if 'app_guests' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
                            <path fill-rule="evenodd" d="M12 6a3.5 3.5 0 1 0 0 7 3.5 3.5 0 0 0 0-7Zm-1.5 8a4 4 0 0 0-4 4 2 2 0 0 0 2 2h7a2 2 0 0 0 2-2 4 4 0 0 0-4-4h-3Zm6.82-3.096a5.51 5.51 0 0 0-2.797-6.293 3.5 3.5 0 1 1 2.796 6.292ZM19.5 18h.5a2 2 0 0 0 2-2 4 4 0 0 0-4-4h-1.1a5.503 5.503 0 0 1-.471.762A5.998 5.998 0 0 1 19.5 18ZM4 7.5a3.5 3.5 0 0 1 5.477-2.889 5.5 5.5 0 0 0-2.796 6.293A3.501 3.501 0 0 1 4 7.5ZM7.1 12H6a4 4 0 0 0-4 4 2 2 0 0 0 2 2h.5a5.998 5.998 0 0 1 3.071-5.238A5.505 5.505 0 0 1 7.1 12Z" clip-rule="evenodd"/>
                        </svg>
                    </span>
                    {{ 'guest.list.title'|trans }}
                </a>
            </li>
            <li class="mr-2">
                <a href="{{ path('app_tags_list', { eventUuid }) }}" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if 'app_tags' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
                            <path d="M18.045 3.007 12.31 3a1.965 1.965 0 0 0-1.4.585l-7.33 7.394a2 2 0 0 0 0 2.805l6.573 6.631a1.957 1.957 0 0 0 1.4.585 1.965 1.965 0 0 0 1.4-.585l7.409-7.477A2 2 0 0 0 21 11.479v-5.5a2.972 2.972 0 0 0-2.955-2.972Zm-2.452 6.438a1 1 0 1 1 0-2 1 1 0 0 1 0 2Z"/>
                        </svg>
                    </span>
                    {{ 'tag.list'|trans }}
                </a>
            </li>
            <li class="mr-2">
                <a href="{{ path('app_medias_list', { eventUuid }) }}" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if 'app_medias' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m3 16 5-7 6 6.5m6.5 2.5L16 13l-4.286 6M14 10h.01M4 19h16a1 1 0 0 0 1-1V6a1 1 0 0 0-1-1H4a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1Z"/>
                        </svg>
                    </span>
                    {{ 'media.list.title'|trans }}
                </a>
            </li>
            <li class="mr-2">
                <a href="" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if 'app_medias' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 6H4a1 1 0 0 0-1 1v10a1 1 0 0 0 1 1h10a1 1 0 0 0 1-1V7a1 1 0 0 0-1-1Zm7 11-6-2V9l6-2v10Z"/>
                        </svg>
                    </span>
                    Photobooth
                </a>
            </li>
            <li class="mr-2">
                <a href="#" class="inline-flex rounded-t-lg py-4 px-4 text-sm font-medium text-center border-b-2 group {% if '...' in routeName %}{{ activeClass }}{% else %}{{ defaultClass }}{% endif %}">
                    <span class="mr-2">
                        <svg class="w-5 h-5 " aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
                            <path fill-rule="evenodd" d="M8.6 5.2A1 1 0 0 0 7 6v12a1 1 0 0 0 1.6.8l8-6a1 1 0 0 0 0-1.6l-8-6Z" clip-rule="evenodd"/>
                        </svg>
                    </span>
                    En direct
                </a>
            </li>
        </ul>
    </div>
</div>
