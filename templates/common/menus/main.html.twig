{% set activeClass = 'block py-2 pr-4 pl-3 text-white rounded bg-primary-700 lg:bg-transparent lg:text-primary-700 lg:p-0 dark:text-white' %}
{% set defaultClass = 'block py-2 pr-4 pl-3 text-gray-700 border-b border-gray-100 hover:bg-gray-50 lg:hover:bg-transparent lg:border-0 lg:hover:text-primary-700 lg:p-0 dark:text-gray-400 lg:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white lg:dark:hover:bg-transparent dark:border-gray-700' %}

<nav class="bg-white border-gray-200 px-4 lg:px-6 py-2.5 dark:bg-gray-800">
    <div class="flex flex-wrap justify-between">
        <a href="{{ path('app_dashboard') }}" class="flex mr-2">
            <img src="{{ asset('logo.png') }}" class="h-8" alt="Moment Logo" />
            <span class="hidden ml-3 lg:block self-center text-2xl font-semibold whitespace-nowrap dark:text-white">{{ 'common.title'|trans }}</span>
        </a>
        {% include 'common/menus/_user.html.twig' %}
        <div class="hidden items-center w-full lg:flex lg:w-auto lg:order-1" id="toggleMobileMenu">
            <ul class="flex flex-col mt-4 font-medium lg:flex-row lg:space-x-8 lg:mt-0">
                <li>
                    <a href="{{ path('app_dashboard') }}" {% if 'app_dashboard' in routeName %} class="{{ activeClass}}" aria-current="page"{% else %}class="{{ defaultClass}}"{% endif %}>
                        {{ 'dashboard.title'|trans }}
                    </a>
                </li>
                <li>
                    <a href="{{ path('app_events_list') }}" {% if 'app_events' in routeName %} class="{{ activeClass}}" aria-current="page"{% else %}class="{{ defaultClass}}"{% endif %}>
                        {{ 'event.list.title'|trans }}
                    </a>
                </li>
                <li>
                    <a href="#" {% if '...' in routeName %} class="{{ activeClass}}" aria-current="page"{% else %}class="{{ defaultClass}}"{% endif %}>
                        Version Pro
                    </a>
                </li>
            </ul>
        </div>
    </div>
</nav>
